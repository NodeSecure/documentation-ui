  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NodeSecure/documentation-ui/issues"
  },
  "homepage": "https://github.com/NodeSecure/documentation-ui#readme",
  "dependencies": {
    "@nodesecure/flags": "^2.4.0",
    "markdown-it": "^13.0.1"
  },
  "devDependencies": {
    "@nodesecure/eslint-config": "^1.5.0",
    "esbuild": "^0.14.43",
    "http-server": "^14.1.1"
  }
}
