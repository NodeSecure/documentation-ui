{
  "name": "@nodesecure/documentation-ui",
  "version": "1.2.1",
  "description": "Portable UI for NodeSecure tools (guides on flags, security docs etc..)",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "example": "npm run example:build && http-server ./dist",
